services:
  ioc.taskslist:
    build:
      context: C:\git\NetCore2.xDependencyInjectionNative.SimpleAsThat\src
      dockerfile: IoC.TasksList/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: ioctaskslist:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.0/IoC.TasksList.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    volumes:
    - C:\git\NetCore2.xDependencyInjectionNative.SimpleAsThat\src\IoC.TasksList:/app:rw
    - C:\Users\Antonio Filho\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Antonio Filho\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'